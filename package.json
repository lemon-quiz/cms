{
  "name": "with-react-intl",
  "version": "1.0.0",
  "scripts": {
    "dev": "cross-env NODE_ICU_DATA=node_modules/full-icu ts-node --project tsconfig.server.json server.ts",
    "dev-no-custom-server": "next dev",
    "build": "npm run extract:i18n && npm run compile:i18n && next build && tsc -p tsconfig.server.json",
    "extract:i18n": "formatjs extract '{pages,components}/*.{js,ts,tsx}' --format simple --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file lang/nl-NL.json",
    "compile:i18n": "formatjs compile-folder --ast --format simple lang compiled-lang",
    "start": "cross-env NODE_ENV=production NODE_ICU_DATA=node_modules/full-icu node dist/server",
    "start-no-custom-server": "next start"
  },
  "dependencies": {
    "@formatjs/cli": "^2.7.3",
    "@formatjs/intl-datetimeformat": "^2.4.3",
    "@formatjs/intl-getcanonicallocales": "^1.3.2",
    "@formatjs/intl-numberformat": "^5.4.1",
    "@formatjs/intl-pluralrules": "^3.4.0",
    "@formatjs/intl-relativetimeformat": "^7.1.1",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@types/react-beautiful-dnd": "^13.0.0",
    "@types/react-transition-group": "^4.4.1",
    "@types/styled-components": "^5.1.7",
    "@types/uuid": "^8.3.0",
    "accepts": "^1.3.7",
    "animate.css": "^4.1.1",
    "axios": "^0.21.1",
    "babel-plugin-react-intl": "^8.1.1",
    "cross-env": "^7.0.2",
    "full-icu": "^1.3.0",
    "glob": "^7.1.4",
    "material-ui": "^0.20.2",
    "next": "^10.0.5",
    "react": "^17.0.1",
    "react-beautiful-dnd": "^13.0.0",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-intl": "^5.6.3",
    "react-miniverse": "file:../../react-miniverse",
    "react-transition-group": "^4.4.1",
    "rxjs": "^6.6.3",
    "styled-components": "^5.2.1",
    "uuid": "^8.3.2",
    "validator": "^13.5.2"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/accepts": "^1.3.5",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "cross-spawn": "7.0.3",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "prettier": "2.0.5",
    "ts-node": "8.0.1",
    "typescript": "4.0"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": false,
    "endOfLine": "lf",
    "arrowParens": "avoid"
  }
}
